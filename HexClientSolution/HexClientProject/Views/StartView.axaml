<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="HexClientProject.Views.StartView"
             xmlns:vm="clr-namespace:HexClientProject.ViewModels"
             x:DataType="vm:MainWindowViewModel">
    <Grid VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button Content="Open Online Mode" Command="{Binding OpenOnlineMainView}"/>
        <Button Content="Open Local Mode" Command="{Binding OpenLocalMainView}"/>
    </Grid>
</UserControl>