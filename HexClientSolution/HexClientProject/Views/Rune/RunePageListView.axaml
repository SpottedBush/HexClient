<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:views="clr-namespace:HexClientProject.Views.Rune"
             xmlns:rune="clr-namespace:HexClientProject.ViewModels.Rune"
             x:Class="HexClientProject.Views.Rune.RunePageListView"
             x:DataType="rune:RunePageListViewModel">
    <StackPanel>
        <ItemsControl Items="{Binding Pages}">
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <views:RunePageView/>
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
    </StackPanel>
</UserControl>