<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:rune1="clr-namespace:HexClientProject.ViewModels.Rune"
             x:Class="HexClientProject.Views.Rune.RuneEditorView"
             x:DataType="rune1:RuneEditorViewModel">
    <StackPanel Margin="10">
        <TextBox Text="{Binding Name}" />
        <TextBlock Text="Primary Tree:" />
        <TextBlock Text="{Binding PrimaryTree.TreeName}" />
        <TextBlock Text="Secondary Tree:" />
        <TextBlock Text="{Binding SecondaryTree.TreeName}" />
    </StackPanel>
</UserControl>